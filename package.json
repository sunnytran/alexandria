{
  "name": "reactexpress",
  "version": "1.0.0",
  "description": "Starter kit for creating React/Redux and Express apps",
  "main": "server.js",
  "scripts": {
    "client-install": "cd client && npm install",
    "start": "npm run watch:css & node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "build:css": "postcss client/src/styles/tailwind.css -o client/src/styles/main.css",
    "watch:css": "postcss client/src/styles/tailwind.css -o client/src/styles/main.css"
  },
  "author": "Brad Traversy & Gregory Assasie",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^9.7.6",
    "postcss-cli": "^7.1.2",
    "tailwindcss": "^1.8.10"
  },
  "dependencies": {
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "cloudinary": "^1.23.0",
    "cloudinary-react": "^1.6.6",
    "concurrently": "^4.1.2",
    "datauri": "^3.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-flash": "0.0.2",
    "express-session": "^1.17.1",
    "knex": "^0.21.5",
    "multer": "^1.4.2",
    "nodemon": "^1.19.1",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "pg": "^8.3.2",
    "redux-thunk": "^2.3.0",
    "seedrandom": "^3.0.5"
  }
}
